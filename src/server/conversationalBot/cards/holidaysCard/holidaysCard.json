{
    "type": "AdaptiveCard",
    "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
    "version": "1.3",
    "body": [
        {
            "type": "ColumnSet",
            "columns": [
                {
                    "type": "Column",
                    "width": 1,
                    "items": [
                        {
                            "type": "Image",
                            "url": "data:image/png;base64,",
                            "size": "Large"
                        }
                    ]
                },
                {
                    "type": "Column",
                    "width": 2,
                    "items": [
                        {
                            "type": "TextBlock",
                            "text": "Holidays & Vacations",
                            "wrap": true,
                            "weight": "Bolder",
                            "fontType": "Default",
                            "size": "Medium"
                        },
                        {
                            "type": "TextBlock",
                            "text": "Altimetrik employees have 16 days per year and 2 floating days.",
                            "wrap": true
                        },
                        {
                            "type": "TextBlock",
                            "text": "Check holidays granted by Altimetrik Mexico",
                            "wrap": true
                        }
                    ]
                }
            ]
        }
    ],
    "actions": [
        {
            "type": "Action.OpenUrl",
            "title": "Holidays & Vacations",
            "url": "https://o365altimetrik.sharepoint.com/sites/AltimetrikMexico/SitePages/Vacaciones.aspx",
            "style": "positive"
        }
    ]
}